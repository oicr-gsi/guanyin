{
  "name": "guanyin",
  "version": "1.4.0",
  "description": "Report DB",
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "start:prod": "NODE_ENV=production node --abort-on-uncaught-exception ./bin/www",
    "fw:clean": "docker run --rm -v $(pwd)/conf:/flyway/conf -v $(pwd)/sql:/flyway/sql --network=host flyway/flyway clean",
    "fw:migrate": "docker run --rm -v $(pwd)/conf:/flyway/conf -v $(pwd)/sql:/flyway/sql --network=host flyway/flyway migrate",
    "pretest": ". test/setup_test_db.sh",
    "posttest": ". test/stop_test_dbs.sh",
    "test": "mocha --exit",
    "lint": "eslint .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git"
  },
  "author": "Xuemei Luo",
  "license": "MIT",
  "dependencies": {
    "ajv": "^8.12.0",
    "bluebird": "^3.7.2",
    "body-parser": "~1.20.0",
    "cookie-parser": "~1.4.6",
    "cookiejar": "^2.1.4",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "joi": "~17.7.0",
    "JSONStream": "^1.3.5",
    "morgan": "^1.10.0",
    "pg": "^8.9.0",
    "pg-promise": "~11.2.0",
    "pg-query-stream": "~4.2.1",
    "prom-client": "^14.1.1",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.2.8",
    "uid": "^2.0.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "qs": "6.5.3"
  }
}
