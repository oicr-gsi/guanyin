{
  "name": "guangyin",
  "version": "1.0.0",
  "description": "Report DB",
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "nodemon ./bin/www",
    "start:prod": "NODE_ENV=production nodejs --abort-on-uncaught-exception ./bin/www",
    "fw:clean": "flyway -X -configFile=conf/flyway.conf clean",
    "fw:migrate": "flyway -X -configFile=conf/flyway.conf -locations=filesystem:./sql/ migrate",
    "fw:test-clean": "flyway -X -configFile=conf/flyway-test.conf clean",
    "fw:test-migrate": "flyway -X -configFile=conf/flyway-test.conf -locations=filesystem:./test/sql/ migrate",
    "test": "PORT=8080 DB_CONNECTION=postgres://test:test@localhost:5432/test_report_db mocha --exit"
  },
  "repository": {
    "type": "git"
  },
  "author": "Xuemei Luo",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^4.0.0",
    "express": "~4.15.5",
    "flywaydb-cli": "^0.5.0",
    "jade": "~1.11.0",
    "morgan": "~1.9.0",
    "pg-promise": "^7.4.0",
    "serve-favicon": "~2.4.5",
    "swagger-jsdoc": "^1.9.7"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.11"
  }
}
